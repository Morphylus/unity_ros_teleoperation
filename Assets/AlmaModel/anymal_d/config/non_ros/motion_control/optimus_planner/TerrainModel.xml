<?xml version="1.0" ?>
<TerrainModel>
  <Parameters frictionCoefficient="0.3"/>
  <MapTopic input="/elevation_mapping/elevation_map_raw" output="/elevation_map_processing/sub_map"/>
  <MapToOdomTFFilter dt="0.5" timeConstant="0."/>
  <Filters>
    <OutlierFilters>
      <Median enable="true" kernelSize="3"/>
      <Opening enable="true" kernelSize="3" numIterations="1"/>
      <Closing enable="true" kernelSize="3" numIterations="1"/>
    </OutlierFilters>
    <InPaint radius="0.05"/>
    <SurfaceNormals kernelSize="0.08" filterSize="0.35"/>
    <SmoothElevation kernelSize="1.0"/>
    <SmoothNormals kernelSize="0.65"/>
  </Filters>
</TerrainModel>
