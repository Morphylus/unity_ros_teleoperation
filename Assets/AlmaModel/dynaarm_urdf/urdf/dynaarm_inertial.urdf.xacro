                                        iyy="${1849*mm2_to_m2}" izz="${115*mm2_to_m2}"
                                        ixy="${0.0*mm2_to_m2}" ixz="${0.0*mm2_to_m2}"    iyz="${0.0*mm2_to_m2}" />
                        </xacro:unless>
                        <xacro:if value="${simulation_property}">
                                <inertia ixx="${inertia_factor_forearm_xx*1850*mm2_to_m2}"
                                        iyy="${inertia_factor_forearm_yy*1849*mm2_to_m2}"
                                        izz="${inertia_factor_forearm_zz*115*mm2_to_m2}"
                                        ixy="${0.0*mm2_to_m2}" ixz="${0.0*mm2_to_m2}"    iyz="${0.0*mm2_to_m2}" />
                        </xacro:if>
                </inertial>
        </xacro:unless>
        <!-- exported values from meshlab. scaled accordingly -->
        <xacro:unless value="${link_nbr-link04_4tube_realsense_nbr}">
