                        <inertia ixx="${1161.79*mm2_to_m2}"
                                iyy="${46092.126*mm2_to_m2}" izz="${46279.53*mm2_to_m2}"
                                ixy="${-0.72*mm2_to_m2}" ixz="${-140.45*mm2_to_m2}"
                                iyz="${0.007*mm2_to_m2}" />
                </inertial>
        </xacro:unless>

        <!-- *************************************************** -->
        <!-- *************** elbow CAD datas. *************** -->
