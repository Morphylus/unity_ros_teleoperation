<!-- The following inertia factors are used to make the simulation stable -->
<xacro:property name="inertia_factor_forearm_xx" value="5.0"/>
<xacro:property name="inertia_factor_forearm_yy" value="5.0"/>
<xacro:property name="inertia_factor_forearm_zz" value="70.0"/>
<xacro:property name="inertia_factor_wrist_2" value="100.0"/>

<xacro:macro name="dynaarm_inertial" params="link_nbr">

