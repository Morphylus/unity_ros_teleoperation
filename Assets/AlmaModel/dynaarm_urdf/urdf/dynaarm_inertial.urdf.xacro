        <!-- *************** base CAD datas. *************** -->
        <!-- *********************************************** -->
        <xacro:unless value="${link_nbr-base_nbr}">
                <inertial>
                        <mass value="${base_mass}" />
                        <origin xyz="${0.026302343*mm_to_m} ${-0.226836219*mm_to_m} ${-30.535730330*mm_to_m}" rpy="0 0 0" />
                        <inertia ixx="${1853.4*mm2_to_m2}"
                                iyy="${1851.4*mm2_to_m2}" izz="${2234.47*mm2_to_m2}"
                                ixy="${-4.535*mm2_to_m2}" ixz="${0.9010*mm2_to_m2}"
                                iyz="${5.559454625*mm2_to_m2}" />
                </inertial>
        </xacro:unless>

        <xacro:unless value="${link_nbr-base_mocap_mount_nbr}">
                <inertial>
                        <mass value="${base_mocap_mount_mass}" />
                        <origin xyz="${0.0*mm_to_m} ${0.0*mm_to_m} ${0.0*mm_to_m}" rpy="0 0 0" />
                        <inertia ixx="${793.652405029*mm2_to_m2}"
                                iyy="${333.473680159*mm2_to_m2}" izz="${933.256614783*mm2_to_m2}"
                                ixy="${0.000681253*mm2_to_m2}" ixz="${-0.000175675*mm2_to_m2}"
                                iyz="${-47.017865545*mm2_to_m2}" />
                </inertial>
        </xacro:unless>

        <!-- *************************************************** -->
        <!-- *************** shoulder CAD datas. *************** -->
        <!-- *************************************************** -->
        <xacro:unless value="${link_nbr-link01_nbr}">
                <inertial>
                        <mass value="${link01_mass}" />
                        <origin xyz="0.0 0.0 ${-1.7336*mm_to_m}" rpy="0 0 0" />
                        <inertia ixx="${10301.21*mm2_to_m2}"
                                iyy="${3016.50*mm2_to_m2}" izz="${10099.04*mm2_to_m2}"
                                ixy="${-50.85*mm2_to_m2}" ixz="${0.0*mm2_to_m2}"
                                iyz="${0.0*mm2_to_m2}" />
                </inertial>
        </xacro:unless>

        <!-- *************************************************** -->
        <!-- *************** upperarm CAD datas. *************** -->
        <!-- *************************************************** -->
        <xacro:unless value="${link_nbr-link02_nbr}">
                <inertial>
                        <mass value="${link02_mass}" />
